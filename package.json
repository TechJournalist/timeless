{
  "name": "stressless",
  "author": "Christian Reinbacher",
  "version": "1.0.0",
  "keywords": [
    "pebble-app"
  ],
  "private": true,
  "dependencies": {
    "enamel": "^1.1.3",
    "pebble-clay": "^1.0.2"
  },
  "pebble": {
    "displayName": "stressless",
    "uuid": "ce33feef-def1-4b3f-b5a7-612493b3cc2c",
    "sdkVersion": "3",
    "enableMultiJS": true,
    "targetPlatforms": [
      "chalk"
    ],
    "watchapp": {
      "watchface": false
    },
    "messageKeys": [
      "border",
      "colorscheme",
      "animation",
      "shake"
    ],
    "resources": {
      "media": [
        {
          "type": "font",
          "characterRegex": "[0-9:]",
          "name": "FONT_DIGITAL_62",
          "file": "fonts/Quirkus.ttf"
        }
      ]
    }
  }
}
